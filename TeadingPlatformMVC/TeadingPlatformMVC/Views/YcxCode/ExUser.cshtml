
@{
    Layout = null;
}
@model IEnumerable<TeadingPlatformMVC.Models.Ycx_User>


@using (Html.BeginForm("UserAdd", "Ycxcode"))
{
    <div>
        <div>
            用户昵称：
            <input type="text" name="UserName" value="" />
        </div>
        <div>
            用户头像：
            <input type="file" name="UserPhoto" value="" />
        </div>
        <div>
            用户性别：
            <input type="radio" name="Sex" value="1" /> 男
            <input type="radio" name="Sex" value="0" />女
        </div>
        <div>
            用户手机号：
            <input type="text" name="UserNumder" value="" />
        </div>
        <div>
            用户年龄：
            <input type="text" name="UserAge" value="" />
        </div>
        <div>
            用户身份证：
            <input type="text" name="UserIDNumber" value="" />
        </div>
        <div>
            <input type="submit" name="name" value="修改" onclick="GetAddReturnMsg()" />


        </div>

    </div>
}
